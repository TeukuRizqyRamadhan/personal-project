<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>My Project </title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="../assets/css/style.css">
</head>

<body>
    <nav class="navbar navbar-expand-lg bg-body-tertiary pt-3">
        <div class="container-fluid">
            <img src="assets/img/brandred.png" alt="logo-dumbways" style="width: 50px;" class="ms-3 me-5">
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link me-3 active" aria-current="page" href="/">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" aria-current="page" href="add-project">Add Project</a>
                    </li>
                </ul>
                <form class="d-flex">
                    <a class="btn btn-dumbways me-3" href="contact" role="button">Contact Me</a>
                </form>
            </div>
        </div>
    </nav>
    <h1 class="text-center mt-5">MY PROJECT</h1>
    <div class="container-fluid  d-flex justify-content-center align-items-center mt-5">
        <div class="row row-cols-1 row-cols-md-3 g-5">
            {{#each projects}}
            <div class="col">
                <div class="card h-100" style="width: 18rem;">
                    <img src="{{this.image}}" class="text-center img-fluidcard-img-top" alt="...">
                    <div class="card-body">
                        <h5 class="card-title">Dumbways 2021</h5>
                        <p class="card-text"><small class="text-body-secondary">Start Date : {{this.start_date}}</small>
                        </p>
                        <p class="card-text"><small class="text-body-secondary">End Date : {{this.end_date}}</small></p>
                        <p class="card-text">{{this.description}}</p>
                        <p class="card-text">{{this.name}}</p>
                        <div class="technology">
                            {{this.technologies}}
                        </div>
                    </div>
                    <div class="card-body">
                        <a class="btn btn-primary" href="#" role="button">Edit</a>
                        <a class="btn btn-primary" href="#" role="button">Delete</a>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>
</body>

</html>