<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Contact Me</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="../assets/css/style.css">
    <link rel="stylesheet" href="../assets/css/testimonial.css">
    <link href='https://fonts.googleapis.com/css?family=Poppins' rel='stylesheet'>
    <script src="https://kit.fontawesome.com/77b392a698.js" crossorigin="anonymous"></script>
</head>

<body>
    <!-- Navbar -->
    <nav class="navbar navbar-expand-lg bg-body-tertiary pt-3">
        <div class="container-fluid">
            <img src="../assets/img/brandred.png" alt="logo-dumbways" style="width: 50px;" class="ms-3 me-5">
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link" aria-current="page" href="/">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" aria-current="page" href="/add-project">Add Project</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="testimonial">Testimonial</a>
                    </li>
                </ul>
                <div class="d-flex">
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                        {{#if isLogin}}
                        <li class="nav-item">
                            <a class="nav-link fw-bold active text-danger">Hello,
                                {{user}}</a>
                        </li>
                        <li class="nav-item ms-3 me-4">
                            <a class="nav-link" href="/logout">Logout</a>
                        </li>
                        {{else}}
                        <li class="nav-item">
                            <a class="nav-link me-3" aria-current="page" href="/login">Login</a>
                        </li>
                        <li class="nav-item me-3">
                            <a class="nav-link" aria-current="page" href="/register">Register</a>
                        </li>
                        {{/if}}
                        <li class="nav-item me-3">
                            <a class="btn btn-dumbways me-3" href="contact" role="button">Contact Me</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>

    <!-- Navbar -->
    <div class="container">
        <h1>My Testimonial</h1>
        <div class="container-rating">
            <h2>Rating by Star</h2>
            <div class="rating">
                <button class="rating-btn" data-rate="0">All</button>
                <button class="rating-btn" data-rate="3">1 <i class="fa-solid fa-star"></i></button>
                <button class="rating-btn" data-rate="2">2 <i class="fa-solid fa-star"></i></button>
                <button class="rating-btn" data-rate="3">3 <i class="fa-solid fa-star"></i></button>
                <button class="rating-btn" data-rate="4">4 <i class="fa-solid fa-star"></i></button>
                <button class="rating-btn" data-rate="5">5 <i class="fa-solid fa-star"></i></button>
            </div>
        </div>
        <div class="container-card">
            <div class="card">
                <img src="https://img001.prntscr.com/file/img001/7c3rRAA7Q_CYU_PslOOtCg.png">
                <div class="content-testi">
                    <div class="desc-testi">
                        <i>"Mantap sekali jasanya!"</i>
                    </div>
                    <div class="name-testi">
                        <p>- Teuku Rizqy Ramadhan</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

</body>
<script src="assets/js/testimonial4.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
    crossorigin="anonymous"></script>

</html>